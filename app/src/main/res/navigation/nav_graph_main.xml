<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph_main" app:startDestination="@id/homeFragment">

    <fragment android:id="@+id/profile">
        <action android:id="@+id/action_profile_to_payment" app:destination="@id/payment"/>
        <action android:id="@+id/action_profile_to_addPen" app:destination="@id/addPen"/>
        <action android:id="@+id/action_profile_to_updateProfile" app:destination="@id/updateProfile"/>
        <action android:id="@+id/action_profile_to_changeReporting" app:destination="@id/changeReporting"/>
    </fragment>
    <fragment android:id="@+id/homeFragment" android:name="com.jasoncavinder.insulinpen_demo.HomeFragment"
              android:label="fragment_home" tools:layout="@layout/fragment_home">
        <action android:id="@+id/action_homeFragment_to_profile" app:destination="@id/profile"/>
        <action android:id="@+id/action_homeFragment_to_payment" app:destination="@id/payment"/>
        <action android:id="@+id/action_homeFragment_to_addPen" app:destination="@id/addPen"/>
        <action android:id="@+id/action_homeFragment_to_updateProfile" app:destination="@id/updateProfile"/>
        <action android:id="@+id/action_homeFragment_to_doseHistory" app:destination="@id/doseHistoryAndWarnings"/>
        <action android:id="@+id/action_homeFragment_to_reporting" app:destination="@id/reporting"/>
        <action android:id="@+id/action_homeFragment_to_penInfo" app:destination="@id/penInfo"/>
    </fragment>
    <fragment android:id="@+id/payment"/>
    <fragment android:id="@+id/addPen"/>
    <fragment android:id="@+id/updateProfile"/>
    <fragment android:id="@+id/doseHistoryAndWarnings" android:name="com.jasoncavinder.insulinpen_demo.DoseHistoryFragment"
              android:label="fragment_dose_list" tools:layout="@layout/fragment_dose_list">
        <action android:id="@+id/action_doseHistoryAndWarnings_to_penInfo" app:destination="@id/penInfo"
        />
    </fragment>
    <fragment android:id="@+id/reporting">
        <action android:id="@+id/action_reporting_to_changeReporting" app:destination="@id/changeReporting"/>
    </fragment>
    <fragment android:id="@+id/penInfo">
        <action android:id="@+id/action_penInfo_to_addPen" app:destination="@id/addPen"/>
    </fragment>
    <fragment android:id="@+id/changeReporting"/>
</navigation>